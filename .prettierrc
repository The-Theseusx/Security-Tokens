{
	"printWidth": 120,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"singleQuote": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"overrides": [
		{
			"files": "*.sol",
			"options": {
				"printWidth": 120,
				"tabWidth": 4,
				"useTabs": true,
				"singleQuote": false,
				"bracketSpacing": true
			}
		},
		{
			"files": "*.json",
			"options": {
				"tabWidth": 2,
				"useTabs": true,
				"parser": "json"
			}
		},
		{
			"files": "*.ts",
			"options": {
				"tabWidth": 2,
				"useTabs": false,
				"parser": "typescript"
			}
		}
	]
}
